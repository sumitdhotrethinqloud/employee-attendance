{
  "name": "Employee Attendance Logger",
  "description": "Track employee check-in and check-out with geolocation and timestamps.",
  "version": "1.0.0",
  "integration": {
    "access_scopes": [
      "boards:read",
      "boards:write",
      "account:read",
      "updates:write",
      "users:read"
    ]
  },
  "features": [
    {
      "name": "Main App View",
      "kind": "board_view",
      "url": "https://tq-monday-employee-attendance1.netlify.app/",
      "method": "iframe"
    },
    {
      "name": "Settings",
      "kind": "app_settings",
      "url": "https://tq-monday-employee-attendance1.netlify.app/settings",
      "method": "iframe"
    }
  ]
}
